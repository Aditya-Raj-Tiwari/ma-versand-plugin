																																																		    var $carousel = $('.hero-section-seven > .list-item-carousel');
																																																		
																																																		    $carousel.owlCarousel({
																																																		        center: true, 
																																																		        items: 5,
																																																		        autoHeight: true,
																																																		        dots: false,
																																																		        loop: true,
																																																		        margin: 30,
