																																																		        navText: [
																																																		            '<i class="owl-single-item-control fa fa-chevron-left" aria-hidden="true"></i>',
																																																																																																																																								            '<i class="owl-single-item-control fa fa-chevron-right" aria-hidden="true"></i>'
																																																																																																																																								        ],
																																																																																																																																								        responsive: {
																																																																																																																																								            576: {
																																																																																																																																								                items: 2
																																																																																																																																								            },
																																																																																																																																								            768: {
																																																																																																																																								                items: 3
																																																																																																																																								            },
																																																																																																																																								            992: {
																																																																																																																																								                items: 5
																																																																																																																																								            }
																																																																																																																																								        },
																																																																																																																																								        onInitialized: scaleItems,
																																																																																																																																								        onResized: scaleItems,
																																																																																																																																								        onTranslate: scaleItems, // Use onTranslate instead of onTranslated
																																																																																																																																								        onTranslated: scaleItems
																																																																																																																																								    });
																																																																																																																																								
																																																																																																																																								    function scaleItems(event) {
																																																																																																																																								        var $activeItems = $carousel.find('.owl-item.active');
																																																																																																																																								        var $allItems = $carousel.find('.owl-item');
																																																																																																																																								
																																																																																																																																								        // Reset scale of all items first
																																																																																																																																								        $allItems.find('.products-overview-carousel').css('transform', 'scale(1)');
																																																																																																																																								
																																																																																																																																								        $activeItems.each(function(index) {
																																																																																																																																								            if(index === 2){ // The middle item
																																																																																																																																								                $(this).find('.products-overview-carousel').css('transform', 'scale(1.8)');
																																																																																																																																								            }
																																																																																																																																								            else if(index === 1 || index === 3){ // The items next to the middle one
																																																																																																																																								                $(this).find('.products-overview-carousel').css('transform', 'scale(1.5)');
																																																																																																																																								            }
																																																																																																																																								        });
																																																																																																																																								    }
																																																																																																																													
																																																																																																																												
																																																																																																																												
																																																																																																																																								};</script2>
