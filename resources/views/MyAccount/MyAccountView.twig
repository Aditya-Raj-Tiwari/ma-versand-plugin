{% extends getPartial('page-design') %}

{% set currentUrl = request.getQueryString() %}

{% block PartialHead %}
	{% embed getPartial('page-metadata') %}
		{% block title trans("Ceres::Template.myAccount") %}
	{% endembed %}
{% endblock %}

{% block PageBody %}

	{{ component( "Ceres::Customer.Components.AddressSelect.AddressSelect" ) }}
	{{ component( "Ceres::MyAccount.Components.AccountSettings" ) }}
	{{ component( "Ceres::MyAccount.Components.History" ) }}
	{{ component( "Ceres::MyAccount.Components.BankDataSelect" ) }}

	{% set userObject = services.customer.getContact() %}
	{% set userBankData = services.contactBank.getBanksOfContact(frontend.Account.AccountContactId) %}
	{% set isReturnActive = ceresConfig.myAccount.orderReturnActive %}
	{% set contactHasReturns = services.customer.hasReturns() %}

	{% set optionalInvoiceAddressFields = {
        "de": ceresConfig.addresses.billingAddressShow,
        "gb": ceresConfig.addresses.billingAddressShow_en
        }
    %}
	{% set requiredInvoiceAddressFields = {
        "de": ceresConfig.addresses.billingAddressRequire,
        "gb": ceresConfig.addresses.billingAddressRequire_en
        }
    %}

	{% set optionalShippingAddressFields = {
        "de": ceresConfig.addresses.deliveryAddressShow,
        "gb": ceresConfig.addresses.deliveryAddressShow_en
        }
    %}
	{% set requiredShippingAddressFields = {
        "de": ceresConfig.addresses.deliveryAddressRequire,
        "gb": ceresConfig.addresses.billingAddressRequire_en
        }
    %}

	<my-account :delivery-address-list="{{ services.customer.getAddresses(2) | json_encode }}" :selected-delivery-address="{{ services.checkout.getDeliveryAddressId() | json_encode }}" :billing-address-list="{{ services.customer.getAddresses(1) | json_encode }}" :selected-billing-address="{{ services.checkout.getBillingAddressId() | json_encode }}">
		<div class="page-content myaccount container-max py-5">
			<div
				class="row">
				<!-- ADDRESS AREA -->
				<div class="col-3">
					<h1 class="h2 mb-3 mb-sm-3 mb-md-5 mb-5">{{ trans("Ceres::Template.myAccount") }}</h1>
					<ul class="myaccount-sidebar list-unstyled mb-0">
						<li>
							<a href="/my-account" class="overview {% if not ('order' in currentUrl or 'returns' in currentUrl or 'devicemanager' in currentUrl or 'personaldata' in currentUrl or 'faq' in currentUrl) %}active{% endif %}">Übersicht</a>
						</li>
						<li>
							<a class="order {% if 'order' in currentUrl %}active{% endif %}">Meine Bestellung</a>
						</li>
						<li>
							<a class="returns {% if 'returns' in currentUrl %}active{% endif %}">Meine Retouren</a>
						</li>
						<li>
							<a class="devicemanager {% if 'devicemanager' in currentUrl %}active{% endif %}">Meine Geräte</a>
						</li>
						<li>
							<a href="/wish-list">Meine Lieblingsprodukte</a>
						</li>
						<li>
							<a class="personaldata {% if 'personaldata' in currentUrl %}active{% endif %}">Persönliche Daten</a>
						</li>
						<li>
							<a class="faq {% if 'faq' in currentUrl %}active{% endif %}">Hilfe & FAQ</a>
						</li>
					</ul>
				</div>
				<!-- ./ADDRESS AREA -->
				<!-- ACCOUNT SETTINGS AREA -->
				<div class="col-9">
					<div class="d-flex flex-wrap ml-0 ml-sm-0 ml-md-5 ml-lg-5">
						<div class="row w-100 mx-0 mb-3 mb-sm-3 mb-md-5 mb-5">
							<div class="col-6">
								{% if frontend.Account.getIsAccountLoggedIn %}
									{% if userObject.firstName|length > 0 and userObject.lastName|length > 0 %}
										<span class="h2">{{ trans("Ceres::Template.myAccountHello", {"username": userObject.firstName }) }}</span>
									{% else %}
										<span class="h2">{{ trans("Ceres::Template.myAccountHello", {"username": userObject.email}) }}</span>
									{% endif %}
								{% endif %}
							</div>
							<div class="col-6">
								<a href="#" class="btn btn-danger float-right btn-medium" v-logout>
									<span>{{ trans("Ceres::Template.myAccountLogout") }}</span>
									<i class="fa fa-sign-out"></i>
								</a>
							</div>
						</div>
					</div>
					{% if not ('order' in currentUrl or 'returns' in currentUrl or 'devicemanager' in currentUrl or 'personaldata' in currentUrl or 'faq' in currentUrl) %}
						<div class="myaccount-cards row ml-0 ml-sm-0 ml-md-5 ml-lg-5 mr-0">
							<div class="col-12 col-6 col-md-4 col-lg-4">
								<div class="card mb-4 orders">
									<div class="card-body text-center p-3 p-sm-3 p-md-4 p-lg-5">
										<svg id="Gruppe_216" data-name="Gruppe 216" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="118" height="112" viewbox="0 0 118 112">
											<defs>
												<clipPath id="clip-path">
													<rect id="Rechteck_495" data-name="Rechteck 495" width="118" height="112" fill="none"/>
												</clipPath>
												<pattern id="pattern" preserveaspectratio="none" width="100%" height="100%" viewbox="0 0 144 144">
													<image width="144" height="144" xlink:href="data:image/png;base64,"/>
												</pattern>
											</defs>
											<g id="Gruppe_215" data-name="Gruppe 215" clip-path="url(#clip-path)">
												<rect id="Rechteck_494" data-name="Rechteck 494" width="118" height="112" fill="url(#pattern)"/>
											</g>
										</svg>
										<span class="d-block h4 mb-3">Meine Bestellung</span>
										<span class="d-block">Hier erfahren Sie den aktuellen Status Ihrer Bestellungen</span>
									</div>
								</div>
							</div>
							<div class="col-12 col-6 col-md-4 col-lg-4">
								<div class="card mb-4 returns">
									<div class="card-body text-center p-3 p-sm-3 p-md-4 p-lg-5">
										<svg id="Gruppe_240" data-name="Gruppe 240" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="143" height="147" viewbox="0 0 143 147">
											<defs>
												<clipPath id="clip-path">
													<rect id="Rechteck_511" data-name="Rechteck 511" width="143" height="147" fill="none"/>
												</clipPath>
												<pattern id="pattern" preserveaspectratio="none" width="100%" height="100%" viewbox="0 0 144 144">
													<image width="144" height="144" xlink:href="data:image/png;base64,"/>
												</pattern>
											</defs>
											<g id="Gruppe_239" data-name="Gruppe 239" clip-path="url(#clip-path)">
												<rect id="Rechteck_510" data-name="Rechteck 510" width="143" height="147" fill="url(#pattern)"/>
											</g>
										</svg>
										<span class="d-block h4 mb-3">Meine Retouren</span>
										<span class="d-block">Einblick in Ihre aktuellen oder vergangenen Retouren</span>
									</div>
								</div>
							</div>
							<div class="col-12 col-6 col-md-4 col-lg-4">
								<div class="card mb-4 personaldata">
									<div class="card-body text-center p-3 p-sm-3 p-md-4 p-lg-5">
										<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="123" height="130" viewbox="0 0 123 130">
											<defs>
												<clipPath id="clip-path">
													<rect id="Rechteck_513" data-name="Rechteck 513" width="123" height="130" transform="translate(-0.328)" fill="none"/>
												</clipPath>
												<pattern id="pattern" preserveaspectratio="none" width="100%" height="100%" viewbox="0 0 144 144">
													<image width="144" height="144" xlink:href="data:image/png;base64,"/>
												</pattern>
											</defs>
											<g id="Gruppe_242" data-name="Gruppe 242" transform="translate(0.328)" clip-path="url(#clip-path)">
												<rect id="Rechteck_512" data-name="Rechteck 512" width="123" height="130" transform="translate(-0.328)" fill="url(#pattern)"/>
											</g>
										</svg>
										<span class="d-block h4 mb-3">Meine Profil</span>
										<span class="d-block">Hier können Sie Ihre persönlichen Daten hinterlegen und ändern</span>
									</div>
								</div>
							</div>
							<div class="col-12 col-6 col-md-4 col-lg-4">
								<div class="card mb-4 devicemanager">
									<div class="card-body text-center p-3 p-sm-3 p-md-4 p-lg-5">
										<svg id="Gruppe_522" data-name="Gruppe 522" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="99" height="99" viewbox="0 0 99 99">
											<defs>
												<clipPath id="clip-path">
													<rect id="Rechteck_922" data-name="Rechteck 922" width="99" height="99" fill="none"/>
												</clipPath>
											</defs>
											<g id="Gruppe_521" data-name="Gruppe 521" clip-path="url(#clip-path)">
												<image id="Rechteck_921" data-name="Rechteck 921" width="99" height="99" xlink:href="data:image/png;base64,"/>
											</g>
										</svg>
										<span class="d-block h4 mb-3">Meine Geräte</span>
										<span class="d-block">Hier können Sie Ihre persönlichen Daten hinterlegen und ändern</span>
									</div>
								</div>
							</div>
							<div class="col-12 col-6 col-md-4 col-lg-4">
								<div class="card mb-4">
									<div class="card-body text-center p-3 p-sm-3 p-md-4 p-lg-5">
										<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="114" height="120" viewbox="0 0 114 120">
											<defs>
												<clipPath id="clip-path">
													<rect id="Rechteck_517" data-name="Rechteck 517" width="114" height="120" transform="translate(0.488)" fill="none"/>
												</clipPath>
												<pattern id="pattern" preserveaspectratio="none" width="100%" height="100%" viewbox="0 0 144 144">
													<image width="144" height="144" xlink:href="data:image/png;base64,"/>
												</pattern>
											</defs>
											<g id="Gruppe_249" data-name="Gruppe 249" transform="translate(-0.488)">
												<g id="Gruppe_248" data-name="Gruppe 248" clip-path="url(#clip-path)">
													<rect id="Rechteck_516" data-name="Rechteck 516" width="114" height="120" transform="translate(0.488)" fill="url(#pattern)"/>
												</g>
											</g>
										</svg>
										<span class="d-block h4 mb-3">Wunschliste</span>
										<span class="d-block">Hier sehen Sie eine Übersicht Ihrer aktuellen Wunschlisten</span>
									</div>
								</div>
							</div>
							<div class="col-12 col-6 col-md-4 col-lg-4">
								<div class="card mb-4 faq">
									<div class="card-body text-center p-3 p-sm-3 p-md-4 p-lg-5">
										<svg id="Gruppe_252" data-name="Gruppe 252" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="111" height="111" viewbox="0 0 111 111">
											<defs>
												<clipPath id="clip-path">
													<rect id="Rechteck_519" data-name="Rechteck 519" width="111" height="111" fill="none"/>
												</clipPath>
											</defs>
											<g id="Gruppe_251" data-name="Gruppe 251" clip-path="url(#clip-path)">
												<image id="Rechteck_518" data-name="Rechteck 518" width="111" height="111" xlink:href="data:image/png;base64,"/>
											</g>
										</svg>
										<span class="d-block h4 mb-3">Hilfe & FAQ</span>
										<span class="d-block">Infos in unserem FAQ-Bereich sowie Kontaktmöglichkeiten</span>
									</div>
								</div>
							</div>
						</div>
					{% endif %}
					{% if 'order' in currentUrl %}
						<div class="row ml-0 ml-sm-0 ml-md-5 ml-lg-5 mr-0">
							<div
								class="col">
								<!-- HISTORY -->
								<orders></orders>
							</div>
						</div>
					{% endif %}
					<!-- Devicemanager -->

					{% if 'devicemanager' in currentUrl %}
						<div class="row ml-0 ml-sm-0 ml-md-5 ml-lg-5 mr-0">
							<div class="col">
								<modal-manager></modal-manager>
							</div>
						</div>
					{% endif %}
					<!-- Devicemanager end -->
					{% if 'personaldata' in currentUrl %}
						<div class="row ml-0 ml-sm-0 ml-md-5 ml-lg-5 mr-0">
							<div
								class="col">
								<!-- ADDRESS AREA -->
								<div class="row">
									<div class="col-md-6">
										<h4>
											{{ trans("Ceres::Template.myAccountInvoiceAddresses") }}
										</h4>
										<p class="text-muted small font-italic">{{ trans("Ceres::Template.myAccountAll") }}
											{{ trans("Ceres::Template.myAccountInvoiceAddresses") }}</p>
										<div class="invoice-addresses-select">
											<invoice-address-select :optional-address-fields="{{ optionalInvoiceAddressFields | json_encode }}" :required-address-fields="{{ requiredInvoiceAddressFields | json_encode }}" default-salutation="{{ ceresConfig.addresses.defaultSalutation }}" email="{{ services.customer.getEmail() }}"></invoice-address-select>
										</div>
									</div>
									<div class="col-md-6">
										<h4>
											{{ trans("Ceres::Template.myAccountShippingAddresses") }}
										</h4>
										<p class="text-muted small font-italic">{{ trans("Ceres::Template.myAccountAll") }}
											{{ trans("Ceres::Template.myAccountShippingAddresses") }}</p>
										<div class="shipping-addresses-select">
											<shipping-address-select :optional-address-fields="{{ optionalShippingAddressFields | json_encode }}" :required-address-fields="{{ requiredShippingAddressFields | json_encode }}" :has-any-parcel-box-preset="{{ services.shipping.hasAnyParcelBoxPreset() | json_encode }}" :has-any-post-office-preset="{{ services.shipping.hasAnyPostOfficePreset() | json_encode }}" default-salutation="{{ ceresConfig.addresses.defaultSalutation }}" email="{{ services.customer.getEmail() }}"></shipping-address-select>
										</div>
									</div>
								</div>
								<!-- ./ADDRESS AREA -->

								<!-- ACCOUNT SETTINGS AREA -->
								<div class="row">
									<div class="col-md-6">
										<h4>
											{{ trans("Ceres::Template.myAccountSettings") }}
										</h4>
										<p class="text-muted small font-italic">{{ trans("Ceres::Template.myAccountChangeLoginData") }}</p>

										<div class="card mb-5">
											<account-settings :user-data="{{ userObject | json_encode }}"></account-settings>
										</div>
									</div>

									<div class="col-md-6">
										<h4>
											{{ trans("Ceres::Template.myAccountBankDetails") }}
										</h4>


										<p class="text-muted small font-italic">{{ trans("Ceres::Template.myAccountChangePaymentInformation") }}</p>
										<bank-data-select template="#vue-bank-data-select" :user-bank-data="{{ userBankData }}" contact-id="{{ frontend.Account.AccountContactId }}"></bank-data-select>

									</div>
								</div>
								<!-- ./ACCOUNT SETTINGS AREA -->
							</div>
						</div>
					{% endif %}
				</div>
				<!-- ./ACCOUNT SETTINGS AREA -->
			</div>
		</div>
	</my-account>
	<script2>
		var orderElements = document.getElementsByClassName('order');
																        for (var i = 0; i < orderElements.length; i++) {
																            orderElements[i].addEventListener('click', function(event) {
																                event.preventDefault();
																                var parameterObjekt = { section: 'order' };
																                var parameter = new URLSearchParams(parameterObjekt).toString();
																                var zielURL = 'https://www.ma-versand.de/webshop/kundenkonto?' + parameter;
																                window.location.href = zielURL;
																            });
																        }
																        var orderElements = document.getElementsByClassName('returns');
																        for (var i = 0; i < orderElements.length; i++) {
																            orderElements[i].addEventListener('click', function(event) {
																                event.preventDefault();
																                var parameterObjekt = { section: 'returns' };
																                var parameter = new URLSearchParams(parameterObjekt).toString();
																                var zielURL = 'https://www.ma-versand.de/webshop/kundenkonto?' + parameter;
																                window.location.href = zielURL;
																            });
																        }
																        var orderElements = document.getElementsByClassName('personaldata');
																        for (var i = 0; i < orderElements.length; i++) {
																            orderElements[i].addEventListener('click', function(event) {
																                event.preventDefault();
																                var parameterObjekt = { section: 'personaldata' };
																                var parameter = new URLSearchParams(parameterObjekt).toString();
																                var zielURL = 'https://www.ma-versand.de/webshop/kundenkonto?' + parameter;
																                window.location.href = zielURL;
																            });
																        }
																        var orderElements = document.getElementsByClassName('devicemanager');
																        for (var i = 0; i < orderElements.length; i++) {
																            orderElements[i].addEventListener('click', function(event) {
																                event.preventDefault();
																                var parameterObjekt = { section: 'devicemanager' };
																                var parameter = new URLSearchParams(parameterObjekt).toString();
																                var zielURL = 'https://www.ma-versand.de/webshop/kundenkonto?' + parameter;
																                window.location.href = zielURL;
																            });
																        }
																        var orderElements = document.getElementsByClassName('faq');
																        for (var i = 0; i < orderElements.length; i++) {
																            orderElements[i].addEventListener('click', function(event) {
																                event.preventDefault();
																                var parameterObjekt = { section: 'faq' };
																                var parameter = new URLSearchParams(parameterObjekt).toString();
																                var zielURL = 'https://www.ma-versand.de/webshop/kundenkonto?' + parameter;
																                window.location.href = zielURL;
																            });
																        }
	</script2>
{% endblock %}
